    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectMode">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddPlayer" />
        <activity android:name=".ViewManagerScreen" />
        <activity android:name=".ViewProfitList" />
        <activity android:name=".ViewPrizeList" />
        <activity android:name=".DeletePlayer" />
        <activity android:name=".DeletePlayerConfirmation" />
        <activity android:name=".SetupTournament" />
        <activity android:name=".SetupTournamentConfirmation" />
        <activity android:name=".ViewPlayerPrizes" />
        <activity android:name=".ViewMatches" />
